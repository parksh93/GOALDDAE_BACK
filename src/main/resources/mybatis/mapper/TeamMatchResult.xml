   <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.goalddae.repository.TeamMatchResultRepository">
    <update id="createTeamMatchResultTable" parameterType="java.lang.Long">
        CREATE TABLE team_match_result_#{teamId} (
                                            id BIGINT AUTO_INCREMENT PRIMARY KEY,
                                            team_win VARCHAR(255) NOT NULL,
                                            team_lose VARCHAR(255) NOT NULL,
                                            win_score BIGINT,
                                            lose_score BIGINT,
                                            draw VARCHAR(255) NULL,
                                            team_id BIGINT NOT NULL
        );
    </update>
</mapper>
